<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
	<class name="entities.EvaluationVoting" table="evaluationvotes" catalog="evaluater">
		<id name="evID" type="int">
			<column name="evID" />
			<generator class="identity"/>
		</id>
		<property name="lectureID" type="int">
			<column name="lectureID" not-null="true"/>
		</property>
		<property name="voting" type="int">
			<column name="voting" not-null="true"/>
		</property>
	</class>
</hibernate-mapping>